<!DOCTYPE html>
<html>

<head>
    <title>School Share</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'parent', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
    <% content_for :content do %>
    <%= render 'parent/tops/header' %>
    <%= yield %>
    <%= render 'parent/tops/footer' %>
    <% end %>
    <%= render template: "layouts/application" %>
</body>

</html>